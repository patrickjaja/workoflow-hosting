# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres

# Minio
MINIO_ROOT_PASSWORD=miniopassword
MINIO_ROOT_USER=miniousername

# N8N
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=postgres
DB_POSTGRESDB_USER=postgres
DB_POSTGRESDB_PASSWORD=postgres
N8N_DIAGNOSTICS_ENABLED=false
N8N_PERSONALIZATION_ENABLED=false
N8N_HOST=localhost
WEBHOOK_URL=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_METRICS=true
N8N_RUNNERS_ENABLED=true
QUEUE_HEALTH_CHECK_ACTIVE=true
NODE_ENV=production
EXECUTIONS_MODE=queue
QUEUE_BULL_REDIS_HOST=redis

# Worker & Execution Settings
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true

# Security Settings
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true

# WORKOFLOW
WORKOFLOW_N8N_WEBHOOK_URL=xxxxxx
MicrosoftAppType=
MicrosoftAppId=xxxxxxx
MicrosoftAppPassword=xxxxxxxx
MicrosoftAppTenantId=4xxxxxxxxxx

#SEARXNG
SEARXNG_SECRET_KEY=xxx

# Task Runner Configuration (v2.0 preparation)
N8N_RUNNERS_MODE=external
N8N_RUNNERS_AUTH_TOKEN=change-me-dev-token
N8N_RUNNERS_BROKER_LISTEN_ADDRESS=0.0.0.0

# LiteLLM Proxy Configuration
LITELLM_MASTER_KEY=sk-workoflow-dev-master-key-change-me
LITELLM_SALT_KEY=sk-workoflow-dev-salt-key-change-me

# Azure OpenAI Provider
AZURE_OPENAI_ENDPOINT=https://your-dev-endpoint.openai.azure.com
AZURE_OPENAI_API_KEY=your-dev-api-key
AZURE_OPENAI_DEPLOYMENT=gpt-4.1
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# Additional Azure subscriptions (uncomment when scaling)
# AZURE_OPENAI_ENDPOINT_2=https://your-second-subscription.openai.azure.com
# AZURE_OPENAI_API_KEY_2=your-second-api-key

# Optional providers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
